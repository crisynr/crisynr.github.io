---
import Layout from '../layouts/LayoutMap.astro';
import Map from '../components/MapBrazil.astro';

const title = "H O T E L by N A K A T A";
const description = "BRAZIL";
---

<Layout description={description} title={title}>
    <main>
        <div class="wrap-map" id="map">
            <Map />
        </div>

        <div >         
            <iframe id="frame-list" src="/hotel/hotel-brazil-list"></iframe>
        </div>

    </main>
</Layout>

